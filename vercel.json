{
  "version": 2,
  "builds": [
    {
      "src": "/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "/dist"
      }
    },
    {
      "src": "api/blog/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/blog/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
